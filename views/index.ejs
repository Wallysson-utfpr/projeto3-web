<%-include('cabecalho.ejs')%>
<form method="post">
  <input type="text" name="txtPesquisa" placeholder="Digite o usuario.." />
  <input type="submit" value="Pesquisar" />
</form>
<br />

<table>
  <tr>
    <th>ID</th>
    <th>Nome</th>
    <th>Idade</th>
    <th>Editar</th>
  </tr>

  <% for(usuario of Usuarios) { %>
  <tr>
    <td><%=usuario._id%></td>
    <td><%=usuario.nome%></td>
    <td><%=usuario.idade%></td>
    <td>
      <a href="/del/<%=usuario._id%>">Deletar</a>
    </td>
    <td>
      <a href="/edit/<%=usuario._id%>">Editar</a>
    </td>
  </tr>
  <% } %>
</table>
<%-include('rodape.ejs')%>
